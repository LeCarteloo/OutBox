<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="main.java.entity.UserInfos" table="user_infos" schema="outbox">
        <id name="userInfoId" column="user_infoID"/>
        <property name="name" column="name"/>
        <property name="surname" column="surname"/>
        <property name="eMail" column="e-mail"/>
        <property name="phoneNumber" column="phone_number"/>
        <property name="streetAndNumber" column="street_and_number"/>
        <property name="city" column="city"/>
        <property name="voivodeship" column="voivodeship"/>
    </class>
</hibernate-mapping>