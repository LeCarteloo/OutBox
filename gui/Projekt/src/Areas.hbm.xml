<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="main.java.entity.Areas" table="areas" schema="outbox">
        <id name="areaId" column="areaID"/>
        <property name="name" column="name"/>
        <property name="voivodeship" column="voivodeship"/>
        <property name="city" column="city"/>
        <property name="departmentStreetAndNumber" column="department_street_and_number"/>
    </class>
</hibernate-mapping>